{
  "name": "react-typescript-boilerplate",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.0.3",
    "@apollo/client": "^3.7.1",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^4.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^5.0.2",
    "apollo3-cache-persist": "^0.14.1",
    "enzyme-to-json": "^3.4.4",
    "graphql": "^16.6.0",
    "i18next": "^19.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "gen": "graphql-codegen -r dotenv/config --config './.graphqlrc.yml'",
    "gen:watch": "graphql-codegen --watch",
    "extract": "i18next-scanner --config './src/i18next-scanner.config.js'",
    "storybook": "start-storybook -p 9009 -s .storybook",
    "build-storybook": "build-storybook -s .storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/fragment-matcher": "2.0.1",
